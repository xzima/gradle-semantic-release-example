{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "assigneesFromCodeOwners": true,
  "semanticCommits": "enabled",
  "semanticCommitType": "build",
  "semanticCommitScope": "deps",
  "packageRules": [
    {
      "semanticCommitType": "build",
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "labels": [
    "dependencies"
  ],
  "baseBranches": [
    "develop"
  ],
  "dependencyDashboardApproval": true
}
